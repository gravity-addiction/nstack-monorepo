.videoContainer.noselect.p-0(
  #VideoContainer,
  [ngStyle]!="{'height': (videoService.isFullscreen)?'100%':'inherit'}"
)
  //- .videoDummy.mx-auto(
    #VideoDummyEle,
    *ngIf!="!videoPlayerService.showPlayer",
   )
    img(
      src="assets/images/sdob_player.gif",
      width="100%", height="100%",
      style="width:100%;height:100%;max-width:100%;max-height:100%;display:block;position:relative;"
    )
  
  video.videoPlayer.mx-auto(
    #video,
    [hidden]="!videoPlayerService.showPlayer",
    [preload]="(tokenService.cookiesEnabled)?'metadata':'auto'",
    playsinline,
    webkit-playsinline,
    (error)="onVideoError($event)",
    [ngClass]="{'videoPlayerSelected': this.videoPlayerService.videoSelected}",
    preload
  ) Your browser does not support HTML5 video.

  canvas#canvas.canvas.mx-auto(
    #canvas,
    [hidden]="!videoPlayerService.showPlayer"
  )
